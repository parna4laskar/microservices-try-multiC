{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
  
  	{
      "name": "eureka",
      "image": "parnalaskar/docker-eureka-img",
      "essential": true,
      "memoryReservation": 256,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8761
        }
      ]
    },
    {
      "name": "ms-test",
      "image": "parnalaskar/docker-mstest-img",
	  "hostname":"ms-test",
      "essential": false,
      "memoryReservation": 256,
	  "portMappings": [
        {
          "hostPort": 8080,
          "containerPort": 8080
        }
      ],
	   "links": [
        "eureka"
      ]
    },
    {
      "name": "orchestration",
      "image": "parnalaskar/docker-orchestration-img",
      "essential": false,
      "memoryReservation": 256,
      "portMappings": [
        {
          "hostPort": 8081,
          "containerPort": 8081
        }
      ],
      "links": [
        "ms-test","eureka"
      ]
    }

  ]
}